<?xml version="1.0"?>

<!-- Main Simp TSDS Configuration -->
<config>
    <!-- RabbitMQ host being used by Simp -->
    <rabbitmq ip="127.0.0.1" port="5672" user="guest" password="guest"/>

    <!-- TSDS Credentials -->
    <tsds url="http://127.0.0.1/tsds/services/push.cgi" user="user" password="password" />

    <!-- Separate worker creation to reduce load -->
    <stagger seconds="5"/>
</config>
