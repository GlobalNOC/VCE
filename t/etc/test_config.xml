<accessPolicy>
  <rabbit host="localhost" port="5672" user="guest" pass="guest" exchange="VCE" />
  <switch name="foobar"  ip="managementip" username="username" password="password"
          ssh_port="22" vendor="Brocade" model="MLXe" version="5.8.0"  >

    <port name="eth0/1" owner="edco">
      <tags start="1" end="100" workgroup="edco"/>
      <tags start="101" end="200" workgroup="ajco"/>
    </port>

   <port name="eth0/2" owner="ajco">
      <tags start="1" end="100" workgroup="edco"/>
      <tags start="101" end="200" workgroup="ajco"/>
    </port>

    <commands>
      <system>
        <command type='show' name='show version' interaction='cli'>show version</command>
        <command name='show chassis' type='show' interaction='cli'>show chassis</command>
        <command name='show cpu' type='show' interaction='cli'>show cpu-utilization</command>
      </system>

      <port>
        <command name='show interface' type='show' interaction='cli' >show interface $PORT</command>
        <command name='set port status' input='status' type='action' interaction='netconf'>
          <NETCONF>XML GOES IN HERE</NETCONF>
        </command>

        <command name='set port descr' input='descr' type='action' interaction='cli' configure='true' context='interface $PORT'>port-name $DESCR</command>

      </port>

      <vlan>
        <command type='show' name='show vlan' interaction='cli'>show vlan $VLAN_ID</command>
        <command type='show' name='show vlan statistics' interaction='cli'>show vlan $VLAN_ID statistics</command>
      </vlan>

    </commands>


  </switch>

  <workgroup name="edco"  description="this is edcos exchange point access workgroup">
    <user id="ebalas" />
    <user id="stan" />
  </workgroup>

  <workgroup name="ajco"  description="this is ajcos exchange point access workgroup">
    <user id="aragusa"/>
    <user id="jonstout" />
  </workgroup>

  <workgroup name="admin" admin="1" description="admin workgroup">
    <user id="ebalas" />
  </workgroup>


</accessPolicy>
